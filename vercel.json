{
    "builds": [
        {
            "src": "my_url_shortener/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "my_url_shortener/wsgi.py"
        }
    ],
    "functions": {
        "my_url_shortener/wsgi.py": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}