{
  "functions": {
    "api": {
      "memory": 512,
      "maxDuration": 10,
      "runtime": "vercel-python@0.5.0",
      "entrypoint": "wsgi.handler",
      "env": {
        "DJANGO_SETTINGS_MODULE": "my_url_shortener.settings"
      }
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api"
    }
  ]
}

